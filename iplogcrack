getgenv().Desync_Settings = {
    Modes = {
   Up = {
       enabled = false,
       strengh = 150
   },
          Down = {
       enabled = false,
       strengh = 150
   },
      Right = {
       enabled = false,
       strengh = 150
   },
      Left = {
       enabled = false,
       strengh = 150
   },
