local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Wendigo's Tha Hood Nuker", "BloodTheme")

local Tab = Window:NewTab("Nuker")

local Section = Tab:NewSection("Money Stuff")

Section:NewButton("Wallet Crash", "Cant be turned off,for more effect press alot of times", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/1pSuci7r", true))()
end)
-- used a loadstring cus i didnt want to use RunService
Section:NewButton("FE Infinite money", "yea", function()
do
   local ui = game:GetService("CoreGui"):FindFirstChild("FluxLib")
   if ui then
       ui:Destroy()
   end
end


local FluxLib = loadstring(game:HttpGet("https://pastebin.com/raw/8Eyg2N2M"))()

local win = FluxLib:Window("Tha Hood", "wendigo#4444 was here", Color3.fromRGB(30,144,255), Enum.KeyCode.B)

local Main = win:Tab("FE Infinite Money", "http://www.roblox.com/asset/?id=6023426915")

Main:Toggle("Loop Get Money", "Fires every near ProximityPrompt", false, function(State)
_G.FirePP = State
game:GetService("RunService").Stepped:connect(function()
if _G.FirePP then
       for i, v in pairs(game:GetService("Workspace"):GetDescendants()) do
                   if v:FindFirstChild("ProximityPrompt") then
                       fireproximityprompt(v.ProximityPrompt)
           end
       end
   end
end)
end)

Main:Button("Get Money Once Once", "Fires the ProximityPrompt once", function()
       for i, v in pairs(game:GetService("Workspace"):GetDescendants()) do
                   if v:FindFirstChild("ProximityPrompt") then
                       fireproximityprompt(v.ProximityPrompt)
           end
       end
end)

Section:NewButton("God", "its just god.", function()
    for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do 
    if v.Name == "Death" then 
        v:Destroy() 
    end 
end
end)

local Main = win:Tab("Stuff", "http://www.roblox.com/asset/?id=6023426915")

Main:Button("Get Every Tool", "click 2 times.", function()
    for i,v in pairs(workspace:GetDescendants()) do
        if v:IsA("ClickDetector") then
        fireclickdetector(v)
        end
        end
end)

Main:Button("Name Esp", "esp name", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/violentqm/esp2/main/Esp"))()
end)

FluxLib:Notification("Note: It works only if you are next to an atm,stuff that give u cash", "Alright")
end)

Section:NewButton("Shazam Crash?! (Click Fe Shazam before)", "Nothing.", function()
local args = {
    [1] = "GREEN_SHAZAM:ON"
}
while wait() do
game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
end 
end)

